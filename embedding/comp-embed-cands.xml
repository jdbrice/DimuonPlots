<?xml version="1.0" encoding="UTF-8"?>
<config>

	<Data name="cands" url="export/cand/qa.root" />
	<Data name="embed" url="export/embedding/qa.root" />

	<c>#F00</c>
	<c>#000</c>

	<!-- POSITIVE -->
	<Plot>
		<Histo name="embed/nHitsFit_5" draw="" lw="2" lc="{c[0]}" optstat="0" norm="true" title="#mu^{+}" xtitle="nHitsFit" ytitle="dN/dnHitsFit" logy="0"/>
		<Histo name="cands/d1_mNHitsFit" lw="2" lc="{c[1]}" draw="same" norm="true" />
		<Export url="export/embedding/compare-pos-nHitsFit.pdf" />

		<Legend columns="2" border_size="0" fill_style="0" >
			<Position pos="top left" w="0.3" h="0.2" padding="0.0, 0.0, 0.0, 0.05" />
			<Entry name="embed/nHitsFit_5" title="MC" />
			<Entry name="cands/d1_mNHitsFit" title="DATA" />
		</Legend>
	</Plot>

	<Plot>
		<Histo name="embed/nHitsDedx_5" draw="" lw="2" lc="{c[0]}" optstat="0" norm="true" title="#mu^{+}" xtitle="nHitsDedx" ytitle="dN/dnHitsDedx" logy="0"/>
		<Histo name="cands/d1_mNHitsDedx" lw="2" lc="{c[1]}" draw="same" norm="true" />
		<Export url="export/embedding/compare-pos-nHitsDedx.pdf" />

		<Legend columns="2" border_size="0" fill_style="0" >
			<Position pos="top left" w="0.3" h="0.2" padding="0.0, 0.0, 0.0, 0.05" />
			<Entry name="embed/nHitsDedx_5" title="MC" />
			<Entry name="cands/d1_mNHitsDedx" title="DATA" />
		</Legend>
	</Plot>

	<Plot>
		<Histo name="embed/nHitsRatio_5" draw="" lw="2" lc="{c[0]}" optstat="0" norm="true" title="#mu^{+}" xtitle="nHitsRatio" ytitle="dN/dnHitsRatio" logy="0"/>
		<Histo name="cands/d1_mNHitsRatio" lw="2" lc="{c[1]}" draw="same" norm="true" />
		<Export url="export/embedding/compare-pos-nHitsRatio.pdf" />

		<Legend columns="2" border_size="0" fill_style="0" >
			<Position pos="top left" w="0.3" h="0.2" padding="0.0, 0.0, 0.0, 0.05" />
			<Entry name="embed/nHitsRatio_5" title="MC" />
			<Entry name="cands/d1_mNHitsRatio" title="DATA" />
		</Legend>
	</Plot>

	<Plot>
		<Histo name="embed/dca_5" draw="" lw="2" lc="{c[0]}" optstat="0" norm="true" title="#mu^{+}" xtitle="dca (cm)" ytitle="dN/ddca (cm)^{-1}" logy="1"/>
		<Histo name="cands/d1_mDCA" lw="2" lc="{c[1]}" draw="same" norm="true" />
		<Export url="export/embedding/compare-pos-dca.pdf" />

		<Legend columns="2" border_size="0" fill_style="0" >
			<Position pos="top right" w="0.3" h="0.2" padding="0.0, 0.05, 0.0, 0.0" />
			<Entry name="embed/dca_5" title="MC" />
			<Entry name="cands/d1_mDCA" title="DATA" />
		</Legend>
	</Plot>

	<Plot>
		<Histo name="cands/d1_mNSigmaPion" draw="" lw="2" lc="{c[1]}" optstat="0" norm="true" title="#mu^{+}" xtitle="n#sigma_{#pi}" ytitle="dN/dn#sigma_{#pi}" logy="1"/>
		<Histo name="embed/nspi_5" lw="2" lc="{c[0]}" draw="same" norm="true" />
		<Export url="export/embedding/compare-pos-nsigmapi.pdf" />

		<Legend columns="2" border_size="0" fill_style="0" >
			<Position pos="bottom hcenter" w="0.3" h="0.2" padding="0.0, 0.00, 0.05, 0.0" />
			<Entry name="embed/nspi_5" title="MC" />
			<Entry name="cands/d1_mNSigmaPion" title="DATA" />
		</Legend>
	</Plot>



	<!-- NEGATIVE -->
	<Plot>
		<Histo name="embed/nHitsFit_6" draw="" lw="2" lc="{c[0]}" optstat="0" norm="true" title="#mu^{-}" xtitle="nHitsFit" ytitle="dN/dnHitsFit" logy="0"/>
