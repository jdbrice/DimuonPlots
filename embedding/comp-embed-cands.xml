	<Plot>
		<Histo name="embed/nHitsRatio_6" draw="" lw="2" lc="{c[0]}" optstat="0" norm="true" title="#mu^{-}" xtitle="nHitsRatio" ytitle="dN/dnHitsRatio" logy="0"/>
		<Histo name="cands/d2_mNHitsRatio" lw="2" lc="{c[1]}" draw="same" norm="true" />
		<Export url="export/embedding/compare-neg-nHitsRatio.pdf" />

		<Legend columns="2" border_size="0" fill_style="0" >
			<Position pos="top left" w="0.3" h="0.2" padding="0.0, 0.0, 0.0, 0.05" />
			<Entry name="embed/nHitsRatio_6" title="MC" />
			<Entry name="cands/d2_mNHitsRatio" title="DATA" />
		</Legend>
	</Plot>

	<Plot>
		<Histo name="embed/dca_6" draw="" lw="2" lc="{c[0]}" optstat="0" norm="true" title="#mu^{-}" xtitle="dca (cm)" ytitle="dN/ddca (cm)^{-1}" logy="1"/>
		<Histo name="cands/d2_mDCA" lw="2" lc="{c[1]}" draw="same" norm="true" />
		<Export url="export/embedding/compare-neg-dca.pdf" />

		<Legend columns="2" border_size="0" fill_style="0" >
			<Position pos="top right" w="0.3" h="0.2" padding="0.0, 0.05, 0.0, 0.0" />
